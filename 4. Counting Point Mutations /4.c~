#include<stdio.h>
#define MAX 17

int hamming_dist(char *s, char *t) {
  int i, d;
  for(i = 0; i < MAX; i++) {
    //  printf("s = %s t = %s \n", si, t+i);
    if ((s[i])!=(t[i])) {
      d++;

    }
  }

  return d;
}


int main() {
  int c;
  char a[] = "GAGCCTACTAACGGGAT";
  char b[] = "CATCGTAATGACGGCCT";
  c = hamming_dist(a, b);
  printf("%d \n", c);
  return 0;
}

