#include<stdio.h>

void CountingBases(char *alphabet) {

  int i;
  int adenine = 0;
  int cytosine = 0;
  int guanine = 0;
  int uracyl = 0;
       
  for(i = 0; alphabet[i] != EOF; ++i) {
  
    if(alphabet[i] == 'A') {
      alphabet[i] = 'T';
    }
    if(alphabet[i] == 'C'){
      alphabet[i] = 'A';
    }
    if(alphabet[i] == 'G') { 
      alphabet[i] = 'C';
    }
    if(alphabet[i] == 'T') {
      alphabet[i] = 'A';
    } 
    /*   
    switch(alphabet[i])  { 
    case 'A': ++adenine;
      break;
    case 'C': ++cytosine;
      break;
    case 'G': ++guanine;
      break;
    default: ++uracyl;
      break;
        
    }
    */ 
  }
  for(i = 0; alphabet != EOF; ++i) {
  printf("%s", alphabet);
  }
}


int main() {
  int i;

  char m[] = "UCCCGCUAU";  
  //  printf("%s",m[2]);
  CountingBases(m);

 return 0;
}
